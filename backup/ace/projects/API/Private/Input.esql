BROKER SCHEMA Private


CREATE COMPUTE MODULE API

	DECLARE TransportType  EXTERNAL CHAR 'transport';
	DECLARE MessageType    EXTERNAL CHAR 'message';
	
	CREATE FUNCTION Main() RETURNS BOOLEAN

	BEGIN

		CREATE LASTCHILD OF Environment DOMAIN 'XMLNSC' NAME 'API';
		
		IF EXISTS (Environment.Get[]) THEN
			SET Environment.API = Environment.Get;
			SET Environment.Get = NULL;
			SET OutputRoot = InputRoot;
			RETURN TRUE;
		END IF;		 
		
		SET Environment.API.MsgId         = UUIDASCHAR;
		SET Environment.API.Status        = 'Start';
		SET Environment.API.TransportType = TransportType;
		SET Environment.API.MessageType   = MessageType;
		
		SET Environment.API.Local = InputLocalEnvironment; 
		
		DECLARE I INTEGER 1;
		DECLARE J INTEGER CARDINALITY(InputRoot.*[]);
		
		WHILE I < J DO
			SET Environment.API.Headers.*[I] = InputRoot.*[I];
			SET I = I + 1;
		END WHILE;

		PROPAGATE TO LABEL 'OneWayCheck';
		PROPAGATE TO LABEL 'SetTimeOut';
		
		SET OutputRoot = InputRoot;
				
		RETURN TRUE;

	END;

END MODULE;


CREATE COMPUTE MODULE Sanitize

	CREATE FUNCTION Main() RETURNS BOOLEAN

	BEGIN

		SET OutputRoot.Properties.CodedCharSetId = 1208;
		SET OutputRoot.Properties.Encoding       = 546;
		
		SET OutputRoot.XMLNSC = InputRoot.XMLNSC;
		
		RETURN TRUE;

	END;

END MODULE;


